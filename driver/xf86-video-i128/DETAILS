          MODULE=xf86-video-i128
         VERSION=1.3.5
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$XORG_URL/individual/driver
      SOURCE_VFY=sha1:2cf01b15fc3b5f2588627f051f9ae4d892696854
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20060303
         UPDATED=20120324
           SHORT="The X.Org video driver for Number 9 I128 based cards"

cat << EOF
xf86-video-i128 provides the driver for the Number 9 Imagine (I128)
video cards. The driver is accelerated and provides support for all
versions of the I128 chip family, including the SGI flatpanel
configuration. Multi-head configurations are supported.
EOF
